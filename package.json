{
  "name": "cleanvue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean": "rimraf ../wwwroot/",
    "build-asp:client": "vue-cli-service build --dest ../wwwroot --mode production",
    "build-asp:server": "./node_modules/.bin/cross-env WEBPACK_TARGET=node vue-cli-service build --dest ../wwwroot",
    "build-asp-all": "run-p build-asp:client build-asp:server",
    "build-asp": "./node_modules/.bin/run-s clean build-asp-all"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.3",
    "vue-router": "^3.1.6",
    "vue-router-sync": "^0.1.0",
    "vuex": "^3.1.3",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-pwa": "^4.3.0",
    "@vue/cli-plugin-router": "^4.3.0",
    "@vue/cli-plugin-vuex": "^4.3.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "aspnet-prerendering": "^3.0.1",
    "cross-env": "7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.6.11",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
